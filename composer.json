{
    "require": {
        "slim/slim": "4.*",
        "slim/psr7": "^1.2",
        "php-di/php-di": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
	},
	"autoload": {
		"psr-4": {
			"App\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"App\\Test\\": "tests/"
		}
	},
	"config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
